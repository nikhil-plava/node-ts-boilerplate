{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch via npm dev",
      "preLaunchTask": "npm: dev",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/server.ts",
      "runtimeExecutable": "ts-node",
      "console": "integratedTerminal"
    }
  ]
}
